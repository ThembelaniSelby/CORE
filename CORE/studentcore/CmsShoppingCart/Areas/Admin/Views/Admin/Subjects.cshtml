@model IEnumerable<CmsShoppingCart.Models.Data.Subject>

@{
    ViewBag.Title = "Subjects"; Layout = "~/Views/Shared/AdminLayout.cshtml";
}


<div style="margin-left:20px">

@if (TempData["Success"] != null)
{
    <div class="alert alert-success">
        @TempData["Success"]
    </div>
}
@if (TempData["Error"] != null)
{
    <div class="alert alert-error">
        @TempData["Error"]
    </div>
}
    <h2>Subjects</h2>

    <p>
        <a href="/admin/Admin/addsubject" class="btn btn-success">ADD NEW SUBJECT</a>
    </p>
    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Name)
            </th>
            <th>
                Name(LOWER)
            </th>
           
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Name)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.NameFix)
                </td>

            </tr>
        }

    </table>
</div>
