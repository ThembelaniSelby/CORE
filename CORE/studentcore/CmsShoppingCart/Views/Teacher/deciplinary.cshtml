@model IEnumerable<CmsShoppingCart.Models.Data.AssessmentSecurity>

@{
    ViewBag.Title = "deciplinary";
}

<h2>deciplinary</h2>


<table class="table">
    <tr>
        <th>
            Assessment Number
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Status)
        </th>
        <th>
            Student Email
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.AssId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Status)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.LName)
        </td>
        <td>
            @Html.ActionLink("Disciplined", "Disciplined", new { id=item.Id },new { @class="btn btn-success"}) 
        </td>
    </tr>
}

</table>
