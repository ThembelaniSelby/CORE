@model IEnumerable<CmsShoppingCart.Models.Data.Gradesubject>

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
</head>
<body>
    <h1>GRADE SUBJECTS</h1>
  
    <table class="table">
        <tr>
            <th>
                Subject Name
            </th>
            <th>
                Subject Name(LOWER)
            </th>
            <th>
                Grade Name
            </th>
            <th>
               Grade Number
            </th>
            <th></th>
        </tr>
    
    @foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.SubjectName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SubjectNameFix)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.GradeName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.GradeNumber)
            </td>
            <td>
                <a href="/Admin/Admin/AssignTeacher?id=@item.Id" class="btn btn-success">ASSIGN TEACHER</a>
                @*@Html.ActionLink("Remove Subject", "Delete", new { id=item.Id },new{ @class="btn btn-danger"})*@
            </td>
        </tr>
    }
    
    </table>
</body>
</html>
